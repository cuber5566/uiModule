<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- SquareLayout -->
    <declare-styleable name="SquareLayout">
        <attr name="widthWeight" format="float"/>
        <attr name="heightWeight" format="float"/>
    </declare-styleable>

    <!-- Raised Button -->
    <declare-styleable name="RaisedButton">
        <attr name="rb_backgroundColor" format="color"/>
        <attr name="rb_rippleColor" format="color"/>
        <attr name="rb_enableBackgroundColor" format="color"/>
        <attr name="rb_enableTextColor" format="color"/>
        <attr name="rb_rippleRadius" format="dimension"/>
        <attr name="rb_elevation" format="dimension"/>
        <attr name="rb_rectRadius" format="dimension"/>
    </declare-styleable>

</resources>